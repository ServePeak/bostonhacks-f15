{
	"manifest_version": 2,

	"name": "I'm Feeling Happy!",
	"description": "Never be depressed with this extension.",
	"version": "1.0",
	"icons": { "64" : "images/icon.png" },
	"homepage_url": "https://github.com/ServePeak/bostonhacks-f15",

	"content_scripts": [
		{
			"matches": ["*://*.google.com/*"],
			"js": ["/js/content.js"]
		}
	],
	"permissions": [
		"tabs",
		"activeTab",
		"*://*.google.com/*"
  ]
}
